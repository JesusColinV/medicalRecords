<TextIn@MDTextField>:
    fill_color: 0, 0, 0, 1
    size_hint_x: 0.5
    size_hint: (None,None)
    size: (dp(150),dp(100))



<ContentNavigationDrawer>:
    ScrollView:
        MDList:

            OneLineAvatarListItem:
                text: "LUZ SM"
                IconLeftWidget:
                    icon: ".\\metadata\\LUZ.ico"

            OneLineListItem:
                text: "Paciente"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "scrPacientes"

            OneLineListItem:
                text: "Dashboard"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "scrDashboard"

            OneLineListItem:
                text: "Configuración"
                on_press:
                    root.nav_drawer.set_state("close")
                    root.screen_manager.current = "clearScreen"


